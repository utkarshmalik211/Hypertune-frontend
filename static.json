{
  "root": "build/",
  "https_only": true,
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "https://hypertune-backend.herokuapp.com/"
    },
    "/socket.io/":{
      "origin": "https://hypertune-backend.herokuapp.com/socket.io/"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "*"
    }
  }
}